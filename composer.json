{
    "name": "vynatu/database-config",
    "description": "Overrides the config facade to provide a dual-stack (Database + Filesystem) config system. Checks on the database first and then on the file system",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Felix Lebel",
            "email": "felix@vynatu.io",
            "role": "Co-Founder"
        },
        {
            "name": "Vynatu Cyberlabs, Inc.",
            "homepage": "https://vynatu.io"
        }
    ],
    "keywords": [
        "menu",
        "laravel",
        "php"
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Vynatu\\DatabaseConfig\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "Vynatu\\DatabaseConfig\\ConfigServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

